<?xml version="1.0" encoding="utf-8"?>
<layout>
	<data>
		<variable name="viewModel" type="com.filippovigani.eventvods.viewmodels.EventSectionViewModel" />
	</data>
	<android.support.v7.widget.RecyclerView
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:id="@+id/match_list"
		android:name="com.filippovigani.eventvods.MatchList"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		app:fixedSize="@{true}"
		app:items="@{viewModel.matches}"
		tools:listitem="@layout/match_list_content"
		tools:showIn="@layout/event_section"
		/>
</layout>
